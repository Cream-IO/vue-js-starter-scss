{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@API": ["src/helpers/API.js"],
      "@Config": ["src/config.js"],
      "@Component/*": ["src/components/*"],
      "@View/*": ["src/views/*"],
      "@ViewStyle/*": ["src/scss/views/*"],
      "@ComponentStyle/*": ["src/scss/components/*"],
      "@Asset/*": ["src/assets/*"],
      "@/*": ["src/*"]
    }
  }
}
